{
  "rewrites": [
    {
      "source": "/ingenierie.html",
      "destination": "/ingenierie.html"
    },
    {
      "source": "/secteurs.html",
      "destination": "/secteurs.html"
    },
    {
      "source": "/ia-automatisation.html",
      "destination": "/ia-automatisation.html"
    },
    {
      "source": "/offres.html",
      "destination": "/offres.html"
    },
    {
      "source": "/abonnements-catalogue.html",
      "destination": "/abonnements-catalogue.html"
    }
  ],
  "redirects": [
    {
      "source": "/www.impulcia-afrique.com/:path*",
      "destination": "https://impulcia-afrique.com/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
